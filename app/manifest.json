{
    "manifest_version": 2,
    "name":    "YC Apply Print",
    "description": "extracts the YC application for printing and correction on paper",
    "version": "1.0",
    "browser_action": {
        "default_title": "YC Apply Print",
        "default_icon": "icon.png"
    },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["*://*.ycombinator.com/*"],
        "js":      ["content.js"]
    }],
    "permissions": ["activeTab"]
}